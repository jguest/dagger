{
    "parserOptions": {
        "ecmaVersion": 6
    },

    "env": {
        "browser": true
    },

    "rules": {
        "brace-style": [ "error", "1tbs", { "allowSingleLine": false } ],
        "camelcase": 2,
        "curly": 2,
        "eqeqeq": 2,
        "keyword-spacing": 2,
        "no-unused-vars": 2,
        "no-trailing-spaces": 2,
        "no-var": 2,
        "space-before-function-paren": 2,
        "space-infix-ops": 2,
        "valid-jsdoc": 1
    }
}